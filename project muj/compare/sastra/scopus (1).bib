@ARTICLE{Pichumani2017409,
author={Pichumani, S. and Srinivasan, R.},
title={Study & optimization on the effect of welding current & active flux coating thickness in the weld profile of Al-SiC composite weldment using A-TIG welding},
journal={Journal of Advanced Research in Dynamical and Control Systems},
year={2017},
volume={2017},
number={Special Issue 11},
