}

@CONFERENCE{Dharini2017,
author={Dharini, K. and Bernadette Cynthia, J. and Kamalambikai, B. and Arul Sudar Celestina, J.P. and Muthu, D.},
title={Hazardous E-waste and its impact on soil structure},
journal={IOP Conference Series: Earth and Environmental Science},
year={2017},
volume={80},
number={1},
doi={10.1088/1755-1315/80/1/012057},
art_number={012057},
