@ARTICLE{Satwik201710216,
author={Satwik, K.S.S. and Manikandan, C. and Elamaran, V. and Narasimhan, K. and Raju, N.},
title={An adaptive row-column least significant bit inlay approach for image steganography},
journal={Biomedical Research (India)},
