@CONFERENCE{Sujatha2017,
author={Sujatha, E.R. and Saisree, S. and Prabalini, C. and Aysha Farsana, Z.},
title={Influence of Random Inclusion of Coconut Fibres on the Short term Strength of Highly Compressible Clay},
journal={IOP Conference Series: Earth and Environmental Science},
year={2017},
volume={80},
number={1},
doi={10.1088/1755-1315/80/1/012056},
art_number={012056},
