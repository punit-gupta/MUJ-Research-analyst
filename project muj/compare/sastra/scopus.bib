@CONFERENCE{Kumar2011,
author={Kumar, P.P. and Amirtharajan, R. and Thenmozhi, K. and Rayappan, J.B.B.},
title={Steg-OFDM blend for highly secure multi-user communication},
journal={2011 2nd International Conference on Wireless Communication, Vehicular Technology, Information Theory and Aerospace and Electronic Systems Technology, Wireless VITAE 2011},
