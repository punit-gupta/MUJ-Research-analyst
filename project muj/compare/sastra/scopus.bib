@CONFERENCE{Gayathri2012,
author={Gayathri, M.N. and Himavathi, S. and Sankaran, R.},
title={Performance enhancement of vector controlled drive with rotor flux based MRAS rotor resistance estimator},
journal={2012 International Conference on Computer Communication and Informatics, ICCCI 2012},
year={2012},
doi={10.1109/ICCCI.2012.6158901},
art_number={6158901},
note={cited By 4},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84858723342&doi=10.1109%2fICCCI.2012.6158901&partnerID=40&md5=e40682c6d82924c808458f50720acd96},
affiliation={EEE Department/SEEE, SASTRA University, Thanjavur, India; EEE Department, Pondicherry Engineering College, Pondicherry, India},
abstract={Vector control schemes are increasingly used in induction motor drive systems to obtain high performance. Vector Control transforms the control of an induction motor to that of a separately excited dc motor by creating independent channels for flux and torque control. The successful implementation of the vector control scheme requires the knowledge of the instantaneous position of rotor flux which is measured in the direct vector control scheme and estimated in the indirect vector control scheme. This requires a prior knowledge of the machine parameters which makes the indirect vector control scheme machine parameter dependent especially the rotor resistance. MRAS based rotor resistance estimation is simpler and easy to design. In this Paper, the vector controlled drive with rotor flux based MRAS rotor resistance estimator is designed and validated through MATLAB/SIMULINK. The performance of the vector controlled drive is analyzed extensively and the performance of the estimator is also analyzed in terms of accuracy and settling time. The promising results obtained are presented. © 2012 IEEE.},
correspondence_address1={Gayathri, M.N.; EEE Department/SEEE, SASTRA University, Thanjavur, India; email: nandhini.gayathri@gmail.com},
