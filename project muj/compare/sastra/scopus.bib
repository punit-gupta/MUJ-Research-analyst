year={2013},
doi={10.1109/ICCCNT.2013.6726857},
art_number={6726857},
note={cited By 0},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84894431190&doi=10.1109%2fICCCNT.2013.6726857&partnerID=40&md5=52255be0e393f4374b9bf192edbee609},
