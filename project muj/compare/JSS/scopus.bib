@ARTICLE{Karmarkar201880,
author={Karmarkar, S. and Shashidhar, G.M. and Chauhan, S.},
title={A comparative study of dynamic and static modulus of elasticity of natural fiber reinforced HDPE composites},
journal={Journal of the Indian Academy of Wood Science},
year={2018},
volume={15},
number={1},
pages={80-86},
doi={10.1007/s13196-018-0211-8},
note={cited By 0},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85048718463&doi=10.1007%2fs13196-018-0211-8&partnerID=40&md5=1981c29d125393862fea6d20553e5291},
affiliation={JSS Research Foundation, SJCE Campus, University of Mysore, Mysore, India; Department of Polymer Science and Technology, SJ College of Engineering, JSS Science and Technology University, Mysore, India; Institute of Wood Science and Technology, Bengaluru, India},
abstract={Dynamic modulus of elasticity (DMoE) of natural fiber reinforced high density polyethylene composites prepared with variety of fibers and their different proportions was determined using ultrasonic pulse propagation method. The DMoE was compared with the static modulus of elasticity as determined by both tensile and flexural test. The elastic modulus of the composites increased with the increasing fiber content irrespective of type of fiber. However, the incremental rate in modulus was varying with fiber type. Jute fibers resulted in maximum improvement whereas coir fibers reinforcement had marginal improvement in stiffness of the composites. The elastic modulus was theoretically modelled for all fiber types based on the elastic modulus of the fibers and HDPE and the model was effective in describing the change in modulus with increasing fiber content. There was a strong correlation between DMoE and static MoE (both tensile and flexural modulus). The ratio of DMoE to static MoE was found to vary with fiber content and smaller differences were observed in stiffer composites. This suggests that static MoE was influenced by the visco-elastic behaviour of HDPE and its composites at lower fiber content. © 2018, Indian Academy of Wood Science.},
correspondence_address1={Chauhan, S.; Institute of Wood Science and TechnologyIndia; email: shakti@icfre.org},
publisher={Springer India},
issn={0972172X},
document_type={Article},
source={Scopus},
}

