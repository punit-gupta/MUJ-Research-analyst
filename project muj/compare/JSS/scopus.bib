
@ARTICLE{Shivanna2019276,
author={Shivanna, S. and Subramani, N.K. and Nagaraj, S.K. and Mutturaj, J.R.B. and Basavaraj, S.},
title={Patina-green coloured light emitting polycarbonate films: A synergistic extraction of improved UV endurance and considerate spectral down-conversion},
