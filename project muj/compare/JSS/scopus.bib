@CONFERENCE{Giluka2018395,
author={Giluka, M.K. and Priyadarshi, T. and Kumar, S. and Franklin, A.A. and Tamma, B.R.},
title={An enhanced EAB algorithm to reduce RACH congestion due to IoT traffic in LTE-A networks},
journal={IEEE World Forum on Internet of Things, WF-IoT 2018 - Proceedings},
year={2018},
volume={2018-January},
pages={395-400},
doi={10.1109/WF-IoT.2018.8355156},
note={cited By 1},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85050469287&doi=10.1109%2fWF-IoT.2018.8355156&partnerID=40&md5=54a32c5ba5eb73ba41fa1e3784fc9ea2},
affiliation={Indian Institute of Technology, Hyderabad, India; SJCE, Mysore, India; RVCE, Bangalore, India},
abstract={Increasing IoT traffic in LTE-A networks imposes several issues such as RACH congestion due to large number of devices accessing the network at the same time. EAB (extended access barring) based solutions have been proven to be efficient methods of giving instant relief from RACH congestion in the network. But current EAB solutions are not suitable for the scenario where network traffic is from mixture of light, moderate, and high delay tolerant IoT devices. As a result, these solutions may lead to unnecessary barring of some of the IoT devices. In this paper, we have proposed a noble EAB algorithm which pro-actively monitors the congestion in the network and avoids collective barring of IoT devices. The algorithm has been evaluated based on various metrics such as success rate, average delay, and average backoff. Apart from this, the paper analyses the performance of H2H and M2M devices when moderate congestion reduction methods such as Long-Backoff method, Separate Preamble method, and Slotted Access method are merged with the proposed EAB algorithm. © 2018 IEEE.},
publisher={Institute of Electrical and Electronics Engineers Inc.},
isbn={9781467399449},
document_type={Conference Paper},
source={Scopus},
}

@ARTICLE{Sanjay2018679,
