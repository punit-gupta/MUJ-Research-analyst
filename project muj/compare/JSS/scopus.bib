@CONFERENCE{Manjunath20173620,
author={Manjunath, A. and Ravi, V. and Asundi, S. and Krishnaraj, C. and Shrikanta Aradhya, C.S. and Gattu, N. and Gurumurthy, Y. and Shiva Kumar, P. and Singh, S. and Vishwas, N.M. and Nayak, S. and Amara, Y. and Agrawal, V.K.},
title={Drag enhancement for spacecraft using numerous ultra-thin wires arranged into drag-wire webs of various configurations},
journal={Proceedings of the International Astronautical Congress, IAC},
