@ARTICLE{Shreekanth2018,
author={Shreekanth, T. and Deeksha, M.R. and Kaushik, K.R.},
title={A Novel Data Independent Approach for Conversion of Hand Punched Kannada Braille Script to Text and Speech},
journal={International Journal of Image and Graphics},
year={2018},
volume={18},
