@ARTICLE{Shivalingu20161691,
author={Shivalingu, B.R. and Vivek, H.K. and Priya, B.S. and Soujanya, K.N. and Swamy, S.N.},
title={Purification and characterization of novel fibrin(ogen)olytic protease from Curcuma aromatica Salisb.: Role in hemostasis},
journal={Phytomedicine},
year={2016},
volume={23},
number={13},
pages={1691-1698},
doi={10.1016/j.phymed.2016.09.007},
