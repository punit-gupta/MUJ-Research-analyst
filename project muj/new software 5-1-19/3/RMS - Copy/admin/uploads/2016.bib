@ARTICLE{Vyas2016601,
author={Vyas, G.L. and Prakash, R. and Pal, U.N. and Manchanda, R. and Halder, N.},
title={Penning plasma based simultaneous light emission source of visible and VUV lights},
journal={Plasma Physics Reports},
year={2016},
volume={42},
number={6},
