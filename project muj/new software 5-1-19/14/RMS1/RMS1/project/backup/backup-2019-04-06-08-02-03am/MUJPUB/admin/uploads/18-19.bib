@ARTICLE{Nayak20172159,
author={Nayak, M.K. and Akbar, N.S. and Tripathi, D. and Khan, Z.H. and Pandey, V.S.},
title={MHD 3D free convective flow of nanofluid over an exponentially stretching sheet with chemical reaction},
