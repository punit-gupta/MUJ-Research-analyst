publisher={American Institute of Physics Inc.},
issn={10706631},
coden={PHFLE},
document_type={Article},
source={Scopus},
}

@ARTICLE{Narayanan2016551,
author={Narayanan, K. and Siddiqui, S.A. and Fozdar, M.},
