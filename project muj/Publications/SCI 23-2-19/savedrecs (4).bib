@article{ ISI:000431980000011,
Author = {Yadav, Dinesh and Abegaonkar, Mahesh P. and Koul, Shiban K. and Tiwari,
   Vivekanand and Bhatnagar, Deepak},
Title = {{Two Element Band-Notched UWB MIMO Antenna with High and Uniform
   Isolation}},
Journal = {{PROGRESS IN ELECTROMAGNETICS RESEARCH M}},
Year = {{2018}},
Volume = {{63}},
Pages = {{119-129}},
Abstract = {{In this paper, a single notched band at 5.2 GHz with complementary split
   ring resonator (CSRR) ultra-wideband (UWB) antenna and its
   implementation in designing of two-element multiple-input
   multiple-output (MIMO) with high and uniform isolation are
   experimentally demonstrated. The proposed UWB MIMO consists of a
   T-shaped stub in ground plane in between antenna elements of the size of
   lambda(at3.1) (GHz)/4 to achieve minimum 20 dB isolation, which is
