Title = {{Oxime-modified aluminium(III) alkoxides: Potential precursors for
   gamma-alumina nano-powders and optically transparent alumina film}},
Journal = {{MATERIALS RESEARCH BULLETIN}},
Year = {{2017}},
Volume = {{93}},
Pages = {{373-380}},
Month = {{SEP}},
Abstract = {{Reactions of aluminium(III) iso-propoxide with acetoxime in anhydrous
   benzene yielded complexes of the type
   {[}Al((OPr)-Pr-i)(3-n)(ONC(CH3)(2))(n)], where n =1, 2 or 3. These
